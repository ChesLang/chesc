[Symbol]{
    Space <- " " : "\t",
    NewLine <- "\n",
    LineEnd <- Space* (NewLine : ";"),
}

[Misc]{
    + use Symbol,

    ID <- [a-zA-Z_] [a-zA-Z0-9_]*,
    % ジェネリクス対応 ID
    GenID <- ID (Symbol.Space*# "<"# Symbol.Space*# GenID Symbol.Space*# (","# Symbol.Space*# GenID)* Symbol.Space*# ">"#)?,
}
