[Symbol]{
    + use Comment,
    Space <- " " : "\t",
    NewLine <- "\n",
    LineEnd <- Space* (Comment.Comment : NewLine : ";"),
    SimpleLineEnd <- Space* NewLine,
}

[Comment]{
    + use Symbol,

    Comment <- Symbol.Space* "#" (!Symbol.NewLine .)* Symbol.SimpleLineEnd,
}

[Misc]{
    + use Keyword,

    ID <- !Keyword.Reserved ([a-zA-Z_] [a-zA-Z0-9_]*),
}

[Keyword]{
    Reserved <- "data" : "end" : "enum" : "impl" : "macro" : "struct" : "trait",
}
