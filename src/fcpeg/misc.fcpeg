[Symbol]{
    + use Misc,

    Space <- " " : "\t",
    NewLine <- "\n",
    LineEnd <- Space* (Misc.Comment : NewLine : ";"),
    SimpleLineEnd <- Space* NewLine,
}

[Misc]{
    + use Symbol,
    + use Keyword,

    ID <- !Keyword.Reserved ([a-zA-Z_] [a-zA-Z0-9_]*),
    Comment <- Symbol.Space* "#" (!Symbol.NewLine .)* Symbol.SimpleLineEnd,
}

[Keyword]{
    Reserved <- "as" : "data" : "end" : "enum" : "impl" : "macro" : "struct" : "trait",
}
